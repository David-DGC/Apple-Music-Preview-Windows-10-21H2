/* When a media selection option has a title in its metadata, arrange the language code next to it, e.g. "Director's Commentary - English" */
"SUBTITLE_TRACK_DISPLAY_FORMAT" = "%2$@ - %1$@";

/* Use this as the option's language name if it's missing */
"UNKNOWN_LANGUAGE" = "Sconosciuta";

/* Append one or more characteristics, sometimes with format information, to a legible option's display name, e.g. "English CC Easy Reader" */
"SDH_SUBTITLE_FORMAT" = "%1$@ non udenti";
"EASYTOREAD_SUBTITLE_FORMAT" = "%1$@ Easy Reader";
"CC_SUBTITLE_FORMAT" = "%1$@ non udenti (con riquadro)";
"FORCEDONLY_SUBTITLE_FORMAT" = "%1$@ forzato";

/* Append one or more characteristics, sometimes with format information, to an audible option's display name, e.g. "Russian Overdub Audio Dolby Digital". */
"DESCRIBESVIDEO_AUDIO_FORMAT" = "%1$@ - Descrizioni audio";
"VOICEOVERTRANSLATION_AUDIO_FORMAT" = "Audio %1$@ con sovraincisione"; /* This characteristic will be used for voice overdubs for which a narrator speaks over the main program audio in another language (e.g. Russian, Polish). */
"AUDIOENCODING_AUDIO_FORMAT" = "%1$@ %2$@";
"ORIGINAL_AUDIO_FORMAT" = "Originale: %1$@";
