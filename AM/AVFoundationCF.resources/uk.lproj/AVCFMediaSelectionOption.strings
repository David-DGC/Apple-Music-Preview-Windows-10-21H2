/* When a media selection option has a title in its metadata, arrange the language code next to it, e.g. "Director's Commentary - English" */
"SUBTITLE_TRACK_DISPLAY_FORMAT" = "%1$@ – %2$@";

/* Use this as the option's language name if it's missing */
"UNKNOWN_LANGUAGE" = "Невідомо";

/* Append one or more characteristics, sometimes with format information, to a legible option's display name, e.g. "English CC Easy Reader" */
"SDH_SUBTITLE_FORMAT" = "%1$@ — субтитри";
"EASYTOREAD_SUBTITLE_FORMAT" = "Полегшене читання — %1$@";
"CC_SUBTITLE_FORMAT" = "%1$@ — супровідний текст";
"FORCEDONLY_SUBTITLE_FORMAT" = "%1$@ — примусово";

/* Append one or more characteristics, sometimes with format information, to an audible option's display name, e.g. "Russian Overdub Audio Dolby Digital". */
"DESCRIBESVIDEO_AUDIO_FORMAT" = "%1$@: аудіоопис";
"VOICEOVERTRANSLATION_AUDIO_FORMAT" = "%1$@, закадровий переклад"; /* This characteristic will be used for voice overdubs for which a narrator speaks over the main program audio in another language (e.g. Russian, Polish). */
"AUDIOENCODING_AUDIO_FORMAT" = "%1$@ %2$@";
"ORIGINAL_AUDIO_FORMAT" = "%1$@ (оригінал)";
